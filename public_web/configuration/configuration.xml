<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	
	<loginURL>login.gameleon.co</loginURL>
	
	<siteURL>harendar.gameleon.co</siteURL>
	<siteProtocol>http</siteProtocol>
	<sitePort>80</sitePort>
	
	<areas current="public_web">
		<area>public_web</area>
		<area>public_admin</area>
	</areas>
	
	<compatibleBrowsers>
		<!--
		<browser>
			<name>Opera</name>
			<version>11</version>
		</browser>
		-->
		<browser>
			<name>Explorer</name>
			<version>10</version>
		</browser>
		<browser>
			<name>Safari</name>
			<version>5</version>
		</browser>
		<browser>
			<name>Firefox</name>
			<version>10</version>
		</browser>
		<browser>
			<name>Chrome</name>
			<version>16</version>
		</browser>
	</compatibleBrowsers>
	
	<facebook>
		<appURL><![CDATA[http://apps.facebook.com/bugtopia/]]></appURL>
		<appID><![CDATA[224734744217425]]></appID>
		<permissions><![CDATA[publish_stream]]></permissions>
	</facebook>
	
	<debug>
		<logToServer>true</logToServer>
		<catchJSErrors>false</catchJSErrors>
	</debug>
	
	<cdn>
		<location code="US"><url>http://cdn.harendar.gameleon.co</url></location>
		<location code="RO"><url>http://cdn.harendar.gameleon.co</url></location>
	</cdn>
	
	<localization>
		
		<location code="en-US">
			<dateFormat>d mmm yyyy hh:nn</dateFormat>
		</location>
		
		<location code="ro-RO">
			<dateFormat>d mmm yyyy hh:nn</dateFormat>
		</location>
				
	</localization>
	
</configuration>
